
.first {
	width: 100%;
	height: 70px;
	font-size: 15px;
	background-color: white;
	border-bottom: 1px solid lightgrey;
	display: grid;
	align-items: center; /*侧轴单行居中 */
	justify-items: center;
	/* 分别占 20% 20% 10% 10% 10% 10% 10% 10%*/
	/* 分别占 20% 20% 50% 10%*/
	grid-template-columns: 1fr 1fr 2.5fr 0.5fr;
}
.second {
	width: 100%;
	height: 200px;
	background-color: white;
	border-bottom: 1px solid lightgrey;
	position: relative;
	padding: 8px;
}
.last {
	width: 100%;
	height: 60px;
	background-color: white;
	border-bottom: 1px solid lightgrey;
	font-size: 15px;
	display: grid;
	align-items: center; /*侧轴单行居中 */
	justify-items: center;
	/* 分别占 20% 20% 10% 10% 10% 10% 10% 10%*/
	/* 分别占 20% 20% 50% 10%*/
	grid-template-columns: 1fr 1fr 2.5fr 0.5fr;
}
.final {
	width: 100%;
	position: fixed;
	bottom: 0px;
}

