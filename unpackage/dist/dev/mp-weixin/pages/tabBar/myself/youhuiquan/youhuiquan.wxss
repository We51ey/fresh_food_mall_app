
.main {
	height: 100%;
	background-color: rgb(235, 235, 235);
}
.img {
	position: fixed;
	top: -600rpx;
	bottom: 50rpx;
	margin: auto;
	width: 100%;
	height: 150rpx;
	background-repeat: no-repeat;
	text-align: center;
	z-index: 3;
}
.img_detail {
	width: 150rpx;
	height: 150rpx;
}
.outdate {
	border-radius: 15rpx;
	height: 100%;
	display: flex;
	flex-direction: column;
	justify-content: start;
	padding: 10rpx 0;
	margin: 20rpx 20rpx;
	background: url(data:image/png;base64,) no-repeat 30% 50%/ contain, radial-gradient(circle at 0 50%, transparent 15rpx, white 15rpx) top left,  radial-gradient(circle at 100% 50%, transparent 15rpx, white 15rpx) bottom right;
	background-size: 150rpx 150rpx,50% 100%, 50% 100%;
	background-repeat: no-repeat;
	background-position:50% 50% , 0 50%, 100% 100%;
}
.used {
	border-radius: 15rpx;
	height: 100%;
	display: flex;
	flex-direction: column;
	justify-content: start;
	padding: 10rpx 0;
	margin: 20rpx 20rpx;
	background: url(data:image/png;base64,) no-repeat 30% 50%/ contain, radial-gradient(circle at 0 50%, transparent 15rpx, white 15rpx) top left,  radial-gradient(circle at 100% 50%, transparent 15rpx, white 15rpx) bottom right;
	background-size: 150rpx 150rpx,50% 100%, 50% 100%;
	background-repeat: no-repeat;
	background-position:50% 50% , 0 50%, 100% 100%;
}
.unuse {
	border-radius: 15rpx;
	height: 100%;
	display: flex;
	flex-direction: column;
	justify-content: start;
	padding: 10rpx 0;
	margin: 20rpx 20rpx;
	background:  radial-gradient(circle at 0 50%, transparent 15rpx, white 15rpx) top left,  radial-gradient(circle at 100% 50%, transparent 15rpx, white 15rpx) bottom right;
	background-size: 50% 100%, 50% 100%;
	background-repeat: no-repeat;
	background-position: 0 50%, 100% 100%;
}
.bar {
	width: 100%;
	font-size: 14rpx;
	border-radius: 15rpx 15rpx 0 0;
	background-color: rgb(153, 153, 153);
	margin-top: -10rpx;
	color: rgb(153, 153, 153);
}
.info_top {
	/* border-top: 5rpx rgb(153,153,153) solid; */
	/* background-color: white; */
	border-radius: 15rpx;
	border-bottom: 5rpx rgb(235, 235, 235) dashed;
	padding: 20rpx 40rpx;

	display: flex;
	flex-direction: row;
	justify-content: space-between;
}
.detail_left {
	font-size: 40rpx;
}
.detal_right {
	font-size: 40rpx;
}
.extra {
	color: rgb(153, 153, 153);
	font-size: 30rpx;
}
.yuan {
	display: flex;
	flex-direction: row;
	justify-content: center;
}
.icon-yuan {
	font-size: 20rpx;
	display: flex;
	flex-direction: column;
	justify-content: flex-end;
}
.info_bottom {
	/* background-color: white; */
	border-radius: 15rpx;
	font-size: 25rpx;
	padding: 40rpx 40rpx;
}

